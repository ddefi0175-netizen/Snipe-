{
  "name": "onchainweb",
  "version": "1.0.0",
  "description": "A Web3 dashboard with live crypto prices from CoinGecko and MetaMask wallet integration using ethers.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "npm run clean && vite build --mode production",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "deploy:cloudflare": "npm run build:production && wrangler pages deploy dist --project-name=onchainweb",
    "deploy:workers": "cd .. && wrangler deploy",
    "deploy:all": "npm run deploy:workers && npm run deploy:cloudflare",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "web3",
    "ethereum",
    "react",
    "vite",
    "coingecko",
    "metamask",
    "crypto",
    "defi"
  ],
  "author": "ddefi0175-netizen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddefi0175-netizen/Snipe.git"
  },
  "homepage": "https://github.com/ddefi0175-netizen/Snipe#readme",
  "bugs": {
    "url": "https://github.com/ddefi0175-netizen/Snipe/issues"
  },
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@wagmi/connectors": "^7.1.6",
    "@wagmi/core": "^3.3.2",
    "@walletconnect/universal-provider": "^2.23.1",
    "@web3modal/wagmi": "^5.1.11",
    "firebase": "^11.2.0",
    "qrcode-generator": "^2.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^2.1.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.18.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jest": "^30.2.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^5.4.21",
    "vitest": "^2.1.0",
    "wrangler": "^3.0.0"
  }
}
