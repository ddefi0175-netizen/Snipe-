{
    "version": 2,
    "buildCommand": "cd Onchainweb && npm install && npm run build",
    "outputDirectory": "Onchainweb/dist",
    "installCommand": "cd Onchainweb && npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}